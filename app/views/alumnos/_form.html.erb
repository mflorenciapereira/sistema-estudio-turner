<%= simple_form_for(alumno) do |f| %>
	<%= f.input :nombre %>
	<%= f.input :apellido %>	
	<%= f.input :tipo_documento, as: :autocomplete, label_name: "descripcion" %>		
	<%= f.input :documento %>
	<%= f.input :email %>
	<%= f.input :domicilio %>
	<%= f.input :telefono %>
	<%= f.input :celular %>
	<%= f.input :telefonoContacto %>
	<%= f.input :observaciones, :as => :text %>
	
	
	
	<%= f.button :submit, class: "btn-primary" %>
<% end %>